package entities;

public class Dependentes extends Funcionario{

	private String nome;
	private String dataNascimento;
	
	public Dependentes() {
	}

	
	
	public Dependentes(String nome, String dataNascimento) {
		this.nome = nome;
		this.dataNascimento = dataNascimento;
	}



	public Dependentes(String nomeFuncionario, String dataNascimento, int matricula) {
		super(nomeFuncionario, dataNascimento, matricula);
		this.nome = nome;
		this.dataNascimento = dataNascimento;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getDataNascimento() {
		return dataNascimento;
	}

	public void setDataNascimento(String dataNascimento) {
		this.dataNascimento = dataNascimento;
	}

	
	
	public String toString() {
		return super.getNomeFuncionario(1) + ", dependente: " + nome + ", " + dataNascimento;
	}

	
	
	
	
}
